From ec872c16fb787c826db285969fcba1fea75b97db Mon Sep 17 00:00:00 2001
From: Elliott Sales de Andrade <quantum.analyst@gmail.com>
Date: Sun, 21 Aug 2022 23:39:00 -0400
Subject: [PATCH] Unpin setuptools-scm

We don't have the probelm of invalid versions.

Signed-off-by: Elliott Sales de Andrade <quantum.analyst@gmail.com>
---
 pyproject.toml | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/pyproject.toml b/pyproject.toml
index e7bd1ac..5ca37f8 100644
--- a/pyproject.toml
+++ b/pyproject.toml
@@ -4,7 +4,7 @@
 [build-system]
 requires = [
   "setuptools >= 52",
-  "setuptools-scm[toml] >= 7.0.5",
+  "setuptools-scm[toml]",
   "wheel >= 0.35",
   "pybind11 >= 2.9.1",
   "pybind11 < 2.10.0; python_implementation == 'PyPy'"
